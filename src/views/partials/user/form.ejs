<div class="box-body">
	<div class="form-group">
		<label for="email">E-mail</label>
		<input type="email" name="email" class="form-control" placeholder="Insira seu e-mail" value="<%= data.email %>">
	</div>
	<div class="form-group">
		<label for="password">Senha</label>
		<input type="password" name="password" class="form-control" placeholder="Insira sua senha" value="<%= data.password %>">
	</div>
</div>

<div class="box-footer">
	<button type="submit" class="btn btn-primary">Save</button>
	<a href="/user"   class="btn btn-default">Back</a>	
</div>
